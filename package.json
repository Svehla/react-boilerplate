{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy:build": "aws s3 sync --delete --acl public-read build s3://boilerplate-hello-world-s3-web-dev",
    "d:b": "npm run deploy:build",
    "d": "npm run build && npm run deploy:build",
    "apollo": "apollo client:codegen --target typescript --localSchemaFile=schema.graphql",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "schema": "apollo client:download-schema schema.graphql",
    "l": "npm run lint:fix",
    "lint": "eslint \"{src,scripts}/**/*.{js,ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint \"{src,scripts}/**/*.{js,ts,tsx}\" --fix",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "ts:check-types": "tsc",
    "ts:check-types:watch": "tsc --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "apollo": "^2.32.5",
    "graphql": "^15.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.9",
    "react-scripts": "4.0.3",
    "typed-env-parser": "0.0.8",
    "typescript": "^4.2.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63"
  }
}
